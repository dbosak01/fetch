Package: fetch
Type: Package
Title: Fetch Data from Various Data Sources
Version: 0.1.2
Authors@R: c( 
    person("David", "Bosak", email = "dbosak01@gmail.com", role = c("aut", "cre")),
    person("Raphael", "Huang", email = "chi-hua.huang@astellas.com", role = "ctb"),
